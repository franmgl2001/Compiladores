int x;
int result;
int i[4];

int factorial(int n) {
    if (n <= 1) {
        return 1;
    }
    return n * factorial(n - 1);
}

void main(void) {
    x = 4;
    input(4);
    output();
    result = factorial(5);
    input(result);
    output();

    while (x > 1){
        x = x - 1;
        i[i] = i;
        
        output(i[x])
    }
}